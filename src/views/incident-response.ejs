<%- include('_head', { title: "Incident Response" }) %>
<div class="container">
	<div class="col-sm-8 col-sm-offset-2 rounded" style="background-color: #CCCCCC !important;">
		<% if(incidents.length === 0) { %>
			<div class="row">No incidents found</div>
		<% } %>
		<% for(const incident of incidents) { %>
		<%- include('_incident', { ...incident }) %>
		<% } %>
	</div>
</div>
<%- include('_tail') %>